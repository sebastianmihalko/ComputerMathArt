public class Exponential implements Function
{
  Function function;
  
  public Exponential(Function f)
  {
    function = f;
  }
  
  public double evaluate(double x, double y)
  {
    //input=-1 -> e^(-1) = 1/e -> -1
    //input=1 -> e^1 = e ->1
    Math.exp(4);
    return 1;
  }
  
  public String toString()
  {
    return "exp("+function.toString()+")";
  }
  
}